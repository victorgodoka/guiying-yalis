{
  "name": "guiying-yalis-character-viewer",
  "version": "1.0.0",
  "description": "A detailed character viewer for Guiying Yalis from Honkai Star Rail",
  "main": "dist/script.js",
  "scripts": {
    "build": "esbuild src/script.ts --bundle --minify --outfile=js/script.js --target=es2020",
    "dev": "esbuild src/script.ts --bundle --watch --outfile=js/script.js --target=es2020",
    "start": "live-server",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "honkai-star-rail",
    "character-viewer",
    "typescript",
    "gaming"
  ],
  "author": "Victor Godoka",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "esbuild": "^0.25.5",
    "live-server": "^1.2.2",
    "typescript": "^5.3.0"
  }
}
